---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Inicia en agosto. No es tarde para tu sueño. Líderes en educación a distancia en Argentina. Programas flexibles. ¡Descubre más!" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.ico" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<link rel="stylesheet" href="/estilos/cIStyles.css">
		<meta itemprop="image" content="/scrapping.jpg" />
		<link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap" rel="stylesheet">
		
	</head>
	<body>
		<slot />
        <script src="../../node_modules/flowbite/dist/flowbite"></script> 	
    </body>
</html>
